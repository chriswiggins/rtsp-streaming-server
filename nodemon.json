{
  "env": {
    "DEBUG": "@vgrid/*",
    "DEBUG_COLORS": "yes",
    "NODE_ENV": "development"
  },
  "exec": "DEBUG=rtsp-streaming-server:* babel-node --extensions '.ts' test/index.ts",
